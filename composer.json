{
  "name": "nicolasnf5/dev-salary-calculator-php",
  "type": "project",
  "description": "Dev Salary Calculator (PHP)",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-redis": "*",
    "beberlei/assert": "^3.3",
    "doctrine/dbal": "^2.10",
    "elasticsearch/elasticsearch": "^7.9",
    "fideloper/proxy": "^4.4",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.7",
    "laravel/passport": "^10.1",
    "laravel/telescope": "^4",
    "laravel/ui": "^3.0",
    "league/fractal": "^0.19",
    "league/tactician": "^1",
    "moneyphp/money": "^3.3",
    "myclabs/php-enum": "^1.7",
    "nyholm/psr7": "^1.3",
    "pda/pheanstalk": "^4.0",
    "predis/predis": "^1.1",
    "psr/cache": "^1.0",
    "spatie/period": "^1.5",
    "symfony/psr-http-message-bridge": "^2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.0",
    "fakerphp/faker": "^1.12",
    "mockery/mockery": "^1.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Presentation\\": "presentation/",
      "Application\\": "application/",
      "Infrastructure\\": "infrastructure/",
      "Domain\\": "domain/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
